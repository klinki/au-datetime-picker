{
  "transform": {
    "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.((jsx|tsx|js|ts)?)$",
  "testPathIgnorePatterns": ["build"],
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json"
  ],
  "testEnvironment": "node",
  "setupFiles": [
    "<rootDir>/test/prepare.ts"
  ],
  "modulePaths": [
    "<rootDir>/src",
    "<rootDir>/node_modules"
  ]
}
